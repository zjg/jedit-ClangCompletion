
# general plugin properites
plugin.clangcompletion.ClangCompletionPlugin.name=ClangCompletion
plugin.clangcompletion.ClangCompletionPlugin.author=Doug Rosvick
plugin.clangcompletion.ClangCompletionPlugin.version=0.1
